<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="40dp"
                tools:context=".MainActivity">

    <!--Center-->
    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellCenter"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"/>

    <!--Left-->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellLeft1"
        android:layout_marginRight="3dp"
        android:layout_toLeftOf="@id/cellCenter"
        android:layout_alignTop="@id/cellCenter"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellLeft2"
        android:layout_marginRight="3dp"
        android:layout_toLeftOf="@id/cellLeft1"
        android:layout_alignTop="@id/cellLeft1"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellLeft3"
        android:layout_marginRight="3dp"
        android:layout_toLeftOf="@id/cellLeft2"
        android:layout_alignTop="@id/cellLeft2"/>

    <!--Right-->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellRight1"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/cellCenter"
        android:layout_alignTop="@id/cellCenter"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellRight2"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/cellRight1"
        android:layout_alignTop="@id/cellRight1"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellRight3"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/cellRight2"
        android:layout_alignTop="@id/cellRight2"/>

    <!--Top-->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellTop1"
        android:layout_marginBottom="3dp"
        android:layout_above="@id/cellCenter"
        android:layout_alignLeft="@id/cellCenter"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellTop2"
        android:layout_marginBottom="3dp"
        android:layout_above="@id/cellTop1"
        android:layout_alignLeft="@id/cellTop1"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellTop3"
        android:layout_marginBottom="3dp"
        android:layout_above="@id/cellTop2"
        android:layout_alignLeft="@id/cellTop2"/>

    <!--Bottom-->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottom1"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellCenter"
        android:layout_alignLeft="@id/cellCenter"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottom2"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottom1"
        android:layout_alignLeft="@id/cellBottom1"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottom3"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottom2"
        android:layout_alignLeft="@id/cellBottom2"/>

    <!--Top Left-->

    <!--Top Right-->

    <!--Bottom Left-->

        <!--First Row -->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft01"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellLeft3"
        android:layout_alignLeft="@id/cellLeft3"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft02"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellLeft2"
        android:layout_alignLeft="@id/cellLeft2"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft03"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellLeft1"
        android:layout_alignLeft="@id/cellLeft1"/>


        <!--Second Row -->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft11"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft01"
        android:layout_alignLeft="@id/cellBottomLeft01"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft12"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft02"
        android:layout_alignLeft="@id/cellBottomLeft02"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft13"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft03"
        android:layout_alignLeft="@id/cellBottomLeft03"/>

        <!--Third Row -->

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft21"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft11"
        android:layout_alignLeft="@id/cellBottomLeft11"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft22"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft12"
        android:layout_alignLeft="@id/cellBottomLeft12"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="75dp"
        android:background="#40000000"
        android:id="@+id/cellBottomLeft23"
        android:layout_marginTop="3dp"
        android:layout_below="@id/cellBottomLeft13"
        android:layout_alignLeft="@id/cellBottomLeft13"/>

    <!--Bottom Right-->




</RelativeLayout>
